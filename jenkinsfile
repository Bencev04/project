pipeline {
    agent{label "nginx_tst1"}
    stages{
        stage("Checkout") {
            steps {
                git 'https://github.com/Bencev04/project.git'
                echo "stage 1-Checkout"
                sh "pwd"
            }
        }
        stage("Copy Files to Nginx") {
            steps {
                sh "cp -r * /usr/share/nginx/html"
            }
        }
    }
}